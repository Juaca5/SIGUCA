extends layout

block sigucaCSS
  link(rel='stylesheet', href='/stylesheets/flipclock.css')
  link(rel='stylesheet', href='/stylesheets/sigucaEscritorio.css')
  link(rel='stylesheet' href='/stylesheets/font-awesome.min.css')
block imgSession
  .divSession#divSession
    img#imgSessionPers(src='images/marito.jpg', alt='something') 
    .btn-group.dropup
                button.btn.btn-default.dropdown-toggle#cSession(type='button', data-toggle='dropdown')
                  span.caret
                ul.dropdown-menu
                  li 
                   a(href='/') Cerrar Sesión 
block sigucaNavbarItems
  ul#nav.main
    li
      a.active(href="/escritorio")
        i.icon-home.icon-large
        |&nbsp;Escritorio
    li
      a(href='/graficos')
        i.icon-bar-chart.icon-large
        |&nbsp;Gráficos
    li
      a(href="/justificaciones")
        i.icon-edit.icon-large  
        |&nbsp;Justificaciones
    li
      a(href="/configuracion")
        i.icon-cogs.icon-large
        |&nbsp;Configuracion
    li
      a(href='/Ayuda')
        |Ayuda 
        i.icon-question-sign.icon-large 

block content
  .wrapper
   .grids
    .grid-10.grid.green
      h3 Marcas Personales
      img.img-responsive#grafico(src='/images/tati.PNG', alt='Responsive image') 
    .grid-6.grid.green
     h3 Últimos Eventos   
     ul#tabs.nav.nav-tabs(data-tabs='tabs')
        li.active
          a(href='#permisos', data-toggle='tab') Permisos
        li
          a(href='#justificaciones', data-toggle='tab') Justificaciones
      
      #my-tab-content.tab-content
        #permisos.tab-pane
          p.empleadoAu Federico Mercury 
        #justificaciones.tab-pane
          p.empleadoAu Chucho Valdéz 
      


</div>

    
        
block sigucaJS
  script(src="/javascripts/flipclock/libs/prefixfree.min.js")
  script(src="/javascripts/flipclock/flipclock.min.js")
  script(src="/javascripts/reloj.js")
